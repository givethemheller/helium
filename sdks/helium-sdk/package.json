{
  "name": "@helium/sdk",
  "version": "0.0.1",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "./dist/api.js",
  "types": "./dist/api.d.ts",
  "description": "a compiled API SDK for node.js generated by openApi",
  "scripts": {
    "dev": "yarn run build:watch",
    "transpile": "npx tsc -p ./tsconfig.json",
    "build:watch": "npx tsc -p ./tsconfig.json -w",
    "build": "cross-env node clearBuild.js && npm run transpile"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.3"
  },
  "devDependencies": {
    "@helium/sdk": "^0.0.1",
    "@types/request": "^2.48.4",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^6.0.0",
    "json-loader": "^0.5.7",
    "typescript": "3.8.3",
    "webpack": "4.41.5"
  },
  "resolutions": {}
}