{
  "name": "@helium/ts-tsoa-api",
  "version": "0.0.2",
  "publishConfig": {
    "access": "restricted"
  },
  "description": "Helium API with TSOA generator and openAPI sdk generator",
  "scripts": {
    "dev": "yarn run debug",
    "debug": "cross-env  nodemon --watch '../cbd-shared/build' --watch './src/**/*.ts' --exec ts-node --compiler ttypescript --compiler ttypescript  --files ./src/server.ts --inspect",
    "start": "yarn run transpile && node ./build/server.js",
    "transpile": "yarn ttsc -p ./tsconfig.json",
    "clear-controllers": "node clear-controllers.js",
    "gen-controllers": "ts-node --compiler ttypescript -P ./tsconfig.json contoller-generator.ts",
    "build:watch": "nodemon --watch src --exec \"yarn tsoa:routes && yarn run tsoa:swagger && yarn run gen-api \"",
    "build": "yarn clear-controllers && yarn gen-controllers  && yarn tsoa:routes  && yarn run transpile && yarn run tsoa:swagger && yarn run gen-api",
    "gen-api": "node clear-sdk.js && openapi-generator generate -i ./swagger/swagger.yaml -g typescript-node -o ../../sdks/helium-sdk/src/   --additional-properties supportsES6=true ",
    "tsoa:routes": "yarn tsoa routes -c ./tsoa.json",
    "tsoa:swagger": "yarn tsoa swagger -c ./tsoa.json && yarn cleanSwagger",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "cleanSwagger": "node cleanSwagger.js"
  },
  "bin": {
    "serveCbd": "./cli-serve.js"
  },
  "dependencies": {
    "@helium/api-shared": "^0.0.1",
    "@casl/ability": "3.4.0",
    "@casl/mongoose": "2.3.2",
    "@openapitools/openapi-generator-cli": "^1.0.10-4.2.3",
    "@sendgrid/mail": "6.5.3",
    "async": "^3.1.1",
    "bcrypt": "4.0.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "hashids": "2.2.1",
    "ioredis": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongodb": "3.5.2",
    "mongoose": "5.8.11",
    "mongoose-csv": "^1.1.0",
    "mongoose-id-validator": "^0.5.4",
    "morgan": "^1.9.1",
    "morgan-body": "2.4.8",
    "pg": "^7.9.0",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.15",
    "sitemap": "^5.1.0",
    "superagent": "^5.0.2",
    "twilio": "3.39.5",
    "ts-transformer-keys": "^0.4.1",
    "typeorm": "^0.2.16"
  },
  "devDependencies": {
