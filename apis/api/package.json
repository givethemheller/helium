    "@types/chalk": "^2.2.0",
    "@types/chokidar": "^2.1.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/hashids": "1.0.30",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/ioredis": "^4.0.10",
